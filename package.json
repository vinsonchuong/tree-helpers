{
  "name": "tree-helpers",
  "version": "0.0.0",
  "description": "A set of helpers for creating and manipulating tree data structures",
  "keywords": [
    "tree",
    "parser",
    "data structure"
  ],
  "homepage": "https://github.com/vinsonchuong/tree-helpers",
  "bugs": "https://github.com/vinsonchuong/tree-helpers/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "files": [
    "LICENSE",
    "README.md",
    "src"
  ],
  "main": "src/index.js",
  "bin": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/vinsonchuong/tree-helpers"
  },
  "scripts": {
    "test": "standard --fix && flow && ava",
    "prepare": "build-esm"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-plugin-package-name-import": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "build-esm": "^1.1.3",
    "flow-bin": "^0.41.0",
    "standard-esnext": "^1.0.12"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3",
      "flow"
    ],
    "plugins": [
      "transform-runtime",
      "package-name-import"
    ]
